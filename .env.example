# Discord Bot Token
# Create at https://discord.com/developers/applications
DISCORD_TOKEN=

# OpenRouter API Key
# Get from https://openrouter.ai/keys
OPENROUTER_API_KEY=

# MikroTik Router API Password
# Default user: prometheus
MIKROTIK_PASSWORD=prometheus

# TrueNAS API Key
# Create in TrueNAS UI → Settings → API Keys
TRUENAS_API_KEY=

# Home Assistant Long-Lived Access Token
# Create in HA → Profile → Long-Lived Access Tokens
HA_ACCESS_TOKEN=

# Grafana API Key
# Create in Grafana → Settings → API Keys
GRAFANA_API_KEY=
