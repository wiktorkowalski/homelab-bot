# Discord Bot Token
# Create at https://discord.com/developers/applications
Bot__DiscordToken=

# OpenRouter API Key
# Get from https://openrouter.ai/keys
Bot__OpenRouterApiKey=

# MikroTik Router API Password
# Default user: prometheus
MikroTik__Password=prometheus

# TrueNAS API Key
# Create in TrueNAS UI → Settings → API Keys
TrueNAS__ApiKey=

# Home Assistant Long-Lived Access Token
# Create in HA → Profile → Long-Lived Access Tokens
HomeAssistant__AccessToken=

# Grafana API Key
# Create in Grafana → Settings → API Keys
Grafana__ApiKey=

# Daily Summary
DailySummary__Enabled=true

# Langfuse (LLM observability)
# Get keys from your Langfuse instance → Settings → API Keys
Langfuse__Endpoint=http://langfuse:3000/api/public/otel/v1/traces
Langfuse__PublicKey=
Langfuse__SecretKey=
