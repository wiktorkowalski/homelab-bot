{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "DSharpPlus": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "Bot": {
    "DiscordToken": "",
    "OpenRouterApiKey": "",
    "OpenRouterModel": "google/gemini-3-flash-preview",
    "OpenRouterEndpoint": "https://openrouter.ai/api/v1",
    "DedicatedChannels": [],
    "ExternalUrls": {
      "grafana": "https://grafana.home.vicio.ovh",
      "prometheus": "https://prometheus.home.vicio.ovh",
      "alertmanager": "https://alertmanager.home.vicio.ovh"
    }
  },
  "MikroTik": {
    "Host": "http://192.168.1.1",
    "ApiPort": 8728,
    "Username": "prometheus",
    "Password": ""
  },
  "TrueNAS": {
    "Host": "http://192.168.1.6",
    "ApiKey": ""
  },
  "HomeAssistant": {
    "Host": "http://192.168.1.8:8123",
    "AccessToken": ""
  },
  "Prometheus": {
    "Host": "http://prometheus:9090"
  },
  "Grafana": {
    "Host": "http://grafana:3000",
    "ApiKey": ""
  },
  "Alertmanager": {
    "Host": "http://alertmanager:9093"
  },
  "Loki": {
    "Host": "http://loki:3100"
  },
  "Ntfy": {
    "Host": "http://ntfy:80",
    "DefaultTopic": "alerts"
  },
  "Langfuse": {
    "Enabled": false,
    "Endpoint": "http://langfuse:3000/api/public/otel/v1/traces",
    "PublicKey": "",
    "SecretKey": ""
  }
}
